{
  "name": "waifuvault-node-api",
  "version": "1.9.0",
  "description": "Official Node API for waifuvault.moe",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "include": [
    "src/**/*"
  ],
  "type": "module",
  "scripts": {
    "tsc": "tsc",
    "build": "tsup",
    "start": "node --loader ts-node/esm src/index.ts",
    "lint": "npx eslint .",
    "lint:fix": "npx eslint --fix .",
    "prettier": "npx prettier ./src --check",
    "prettier:fix": "npx prettier ./src --write",
    "prepare": "husky",
    "test": "vitest"
  },
  "keywords": [],
  "author": "Victoria",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@types/node": "^24.3.0",
    "eslint": "^9.34.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "prettier": "3.6.2",
    "ts-node": "^10.9.2",
    "tsc-files": "^1.1.4",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0",
    "vitest": "^3.2.4"
  },
  "homepage": "https://github.com/waifuvault/waifuVault-node-api",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/waifuvault/waifuVault-node-api.git"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "tsc-files --noEmit --pretty",
      "eslint --cache --fix",
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
